clc;
clear;
close all;
scalar = 4;
vector = 0:0.1:1;
row_matrix = [1 2 3];
column_matrix = [1;2;3];
b = 3+6*1i;
c = [1 0 1; -1 1 3; 0 0 4];
c_t = [1 -1 0; 0 1 0; 1 3 4];
d = c+c_t;
e = c-c_t;
f = c*c_t;
square_c = c^2;
A = [2 4 6; 8 10 12; 14 16 18];
C = A.\2;
A_complex = 3+2*1j;
A_real = real(A_complex);
A_imaginary = imag(A_complex);
A_mag = abs(A_complex);
A_phase = angle(A_complex);
Conjugate_A = conj(A_complex);
d_d = A(1,2);
d_e = A(:,2);
